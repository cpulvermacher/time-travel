{
    "env": {
        "browser": true,
        "es2016": true,
        "webextensions": true
    },
    "extends": [
        "eslint:recommended"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "quotes": [
            2,
            "single",
            {
                "avoidEscape": true
            }
        ]
    }
}