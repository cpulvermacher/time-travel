        "typescript": "^5.7.3",
        "typescript-eslint": "^8.24.0",
        "vite": "^6.1.0",
        "vitest": "^3.0.5"
    },
    "dependencies": {
        "svelte": "^5.20.0",
        "@svelte-plugins/datepicker": "^1.0.11"
