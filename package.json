{
    "name": "time-travel",
    "version": "2.4.5",
    "license": "MIT",
    "private": "true",
    "scripts": {
        "build": "sh ./util/build.sh production",
        "dev": "sh ./util/build.sh dev",
        "lint": "tsc && eslint *.mts src/ && prettier src --check && svelte-check --tsconfig ./tsconfig.json && pnpm lint:translations",
        "lint:translations": "bash ./util/check-translations.sh",
        "prettier": "prettier src --write",
        "test": "vitest --run",
        "checkall": "pnpm test && pnpm lint && pnpm build",
        "machine-translate": "inlang machine translate --project project.inlang"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.1",
        "@inlang/cli": "^3.0.12",
        "@inlang/paraglide-js": "^2.5.0",
        "@sveltejs/vite-plugin-svelte": "^6.2.1",
        "@tsconfig/svelte": "^5.0.6",
        "@types/chrome": "^0.1.31",
        "chrome-webstore-upload-cli": "^3.5.0",
        "eslint": "^9.39.1",
        "eslint-plugin-svelte": "^3.13.0",
        "globals": "^16.5.0",
        "happy-dom": "^20.0.11",
        "prettier": "^3.7.2",
        "prettier-plugin-svelte": "^3.4.0",
        "svelte-check": "^4.3.4",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.48.0",
        "vite": "^7.2.4",
        "vitest": "^4.0.14",
        "web-ext": "^9.2.0"
    },
    "dependencies": {
        "@svelte-plugins/datepicker": "^1.0.11",
        "svelte": "^5.46.4"
    },
    "pnpm": {
        "overrides": {
            "brace-expansion@>=1.0.0 <=1.1.11": ">=1.1.12",
            "brace-expansion@>=2.0.0 <=2.0.1": ">=2.0.2"
        }
    }
}
